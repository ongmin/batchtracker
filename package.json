{
  "name": "batchtracker",
  "version": "1.0.0",
  "description": "Purpose: For consumers to check expiry date based on batch numbers",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "start": "npm run build && npm run start-server",
    "start-server": "nodemon dist_server/server.js",
    "build": "npm run clean && mkdir dist && npm run build-html && npm run build-css && npm run build-webpack && npm run build-server",
    "clean": "rm -rf dist && rm -rf dist_server",
    "build-html": "cp app/index.html dist/index.html",
    "build-css": "cp app/main.css dist/bundle.css",
    "build-webpack": "webpack",
    "build-server": "babel server -d dist_server",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ongmin/batchtracker.git"
  },
  "author": "Paula's Choice SG",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ongmin/batchtracker/issues"
  },
  "homepage": "https://github.com/ongmin/batchtracker.git#readme",
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.15.1",
    "cors": "^2.7.1",
    "express": "^4.13.4",
    "express-history-api-fallback": "^2.0.0",
    "fs": "0.0.2",
    "mongoose": "^4.4.14",
    "nodemon": "^1.9.2",
    "path": "^0.12.7",
    "pm2": "^0.15.10",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-router": "^2.4.0",
    "underscore": "^1.8.3"
  },
  "keywords": [
    "paulaschoice",
    "skincare",
    "expiry",
    "batch"
  ],
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "supertest": "^1.2.0",
    "webpack": "^1.13.0"
  }
}
